select x.txid, u.username as order_user, s.name as store_name,x.store_id,x.order_date,x.order_price,x.balance, x.shipped_yn, x.term, x.bankaccount from transaction x , storelocator s, user u, user_group ug where x.store_id = s.id and x.order_user = u.username  and u.user_group_id = ug.user_group_id and ug.user_group_id in (1,11)  AND LCASE(x.order_date) between '2012-01-04' and '2012-01-11' ORDER BY s.accountno DESC